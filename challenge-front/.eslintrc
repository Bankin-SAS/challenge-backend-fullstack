{
  "root": true,
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
